{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vehicles",
      "dest": "/api/vehicles.js"
    },
    {
      "src": "/api/category",
      "dest": "/api/category.js"
    },
    {
      "src": "/api/users",
      "dest": "/api/users.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    }
  ]
}
